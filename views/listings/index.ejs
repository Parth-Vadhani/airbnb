<%-layout("/layouts/boilerplate")%>
<%-include("../includes/navbar2.ejs")%>




<div class="row row-cols-lg-4 row-cols-md-2 row-cols-sm-1 " style="width: 100%;">
                     <%for(let listing of allListings){%>
                            <a href="listings/<%=listing._id%>" class="card-anchor">
                                   <div class="card col">
                                          <img src="<%=listing.image.url%>" class="card-img-top" alt="...">
                                          <div class="card-img-overlay">
                                                 See in Detail
                                          </div>
                                          <div class="card-body">
                                                 <h5 class="card-title">
                                                        <%=listing.title%>
                                                 </h5>
                                                 <p class="card-text">
                                                        &dollar;
                                                        <%=listing.price.toLocaleString("en-US")%>
                                                               / night
                                                 </p>
                                                 <i class="tax">+18% GST</i>
                                                 <!-- <a href="#" class="btn btn-primary">Go somewhere</a> -->
                                          </div>
                                   </div>
                            </a>
                            <%}%>
</div>
       

      